<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="ukesoppgave.sparing.SparingController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label text="Sparing" fx:id="lbltittel"/>
    <Label text="Engangsbeløp" fx:id="txtbelop"/>
    <TextField fx:id="txtBeløp"/>
    <Label text="Sparingsrente (%)" fx:id="txtrente"/>
    <TextField fx:id="txtRente"/>
    <Label text="Antall år" fx:id="txtaar"/>
    <TextField fx:id="txtAntallÅr"/>
    <Button text="Beregn" onAction="#beregn"/>
    <Label fx:id="lblResultat"/>

</VBox>
